<div class="card-body">
    <table id="example2" class="table table-bordered table-hover">
      <thead>
      <tr>
        <th>No</th>
        <th>Nama Koin</th>
        <th>Jumlah Koin</th>
        <th>Harga Koin</th>
      </tr>
      </thead>
      <tbody>
        <% console.log(nominal) %>
        <% for(let i = 0; i < nominal.length; i++) { %>
          <tr>
            <td><%= i + 1 %> </td>
            <td><%= nominal[i].coinName %> </td>
            <td><%= nominal[i].coinQuantity %> </td>
            <td><%= nominal[i].price %> </td>
            <td>
              <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
                <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning">Ubah</a>
                <button type="submit" class="btn btn-danger">Hapus</button>
              </form>
            </td>
          </tr>
        <% } %> 
      </tbody>
    </table>
  </div>